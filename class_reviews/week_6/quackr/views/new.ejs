<%- include("./partials/header") %>

<h1>New Quack</h1>

<form action="/quacks" method="POST">
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="img_url">Image URL</label>
      <input name="img_url" type="text" class="form-control" id="img_url">
    </div>
    <div class="form-group col-md-6">
      <label for="content">Quack Content</label>
      <input name="content" type="text" class="form-control" id="content">
    </div>
  </div>
  <button type="submit" class="btn btn-success">Add Quack</button>
</form>

<%- include("./partials/footer") %>